<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="lanScene" prefHeight="400.0" prefWidth="600.0" styleClass="main" stylesheets="@Game.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.BeginScene">
   <children>
      <Button fx:id="multiButton" layoutX="203.0" layoutY="200.0" mnemonicParsing="false" onAction="#toLanGame" prefHeight="43.0" prefWidth="167.0" text="Multiplayer" />
      <TextField fx:id="nameHolder" layoutX="217.0" layoutY="62.0" onAction="#nameInput" opacity="0.0" />
      <Button fx:id="startButton" layoutX="493.0" layoutY="300.0" mnemonicParsing="false" onAction="#startGame" prefHeight="57.0" prefWidth="103.0" text="Start" />
      <ChoiceBox fx:id="language" layoutX="424.0" layoutY="76.0" prefWidth="150.0" styleClass="choiceBox" stylesheets="@Game.css" value="English">
         <items>
            <FXCollections fx:factory="observableArrayList">
               <String fx:value="English" />
               <String fx:value="Vietnamese" />
            </FXCollections>
         </items>
      </ChoiceBox>
   </children>
</AnchorPane>
